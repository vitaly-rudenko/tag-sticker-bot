{
  "name": "tag-sticker-bot",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon --exec 'npm start'",
    "setup-local": "node src/scripts/setup-local.js",
    "test": "npm run setup-local && npm run jest",
    "jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "tsc"
  },
  "author": "Vitaly Rudenko",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-cloudsearch": "^3.294.0",
    "@aws-sdk/client-dynamodb": "^3.279.0",
    "@aws-sdk/client-ssm": "^3.279.0",
    "dotenv": "^16.0.3",
    "telegraf": "^4.11.2",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "jest": "^29.4.3",
    "jest-extended": "^3.2.4",
    "nodemon": "^2.0.20",
    "typescript": "^5.1.6"
  },
  "nodemonConfig": {
    "watch": [
      "./src",
      "./package-lock.json",
      "./.env"
    ],
    "delay": 500,
    "ext": "js,mjs,json,env"
  }
}
